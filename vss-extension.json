{
    "manifestVersion": 1,
    "id": "tagging",
    "name": "Tag Releases, Pipelines and Git Commits (incl. cascading back)",
    "publisher": "foxholenl",
    "description": "This extension provides tagging for pipelines, releases and git commits. It allows you to tag a git commit from a release pipeline.",
    "version": "1.2.0",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "README.MD"
        }
    },
    "icons": {
        "default": "images/logo.png"
    },
    "links": {
        "support": {
            "uri": "https://github.com/JoostVoskuil/azure-devops-tagging"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/JoostVoskuil/azure-devops-tagging"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "foxholenl-tagging",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}